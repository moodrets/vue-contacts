{
  "name": "vue-contacts",
  "private": false,
  "homepage": "https://github.com/moodrets/vue-contacts",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "copy-404": "cd dist && cp index.html 404.html && cd ..",
    "deploy": "vue-tsc && vite build && npm run copy-404 && gh-pages -d dist"
  },
  "dependencies": {
    "gh-pages": "^5.0.0",
    "material-icons": "^1.13.9",
    "normalize.css": "^8.0.1",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@types/node": "^20.4.6",
    "@vitejs/plugin-vue": "^4.2.3",
    "sass": "^1.64.2",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vue-tsc": "^1.8.5"
  }
}
